{
  "name": "Yundi-RPA",
  "description": "测试插件",
  "version": "1.0.0",
  "manifest_version": 2,
  "browser_action": {
    "default_icon": "icon.png"
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["js/targetSelect.js"],
      "run_at": "document_start",
      "all_frames": true
    }
  ],
  "background": {
    "scripts": ["js/background.js"]
  },
  "permissions": [ "tabs" , "activeTab"] 
}
